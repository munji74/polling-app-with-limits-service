        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "expr": "up{job=~\"api-gateway|user-service|poll-service|limits-service|naming-server|config-service\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "title": "üü¢ Service Health Status",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "color": "red", "index": 1, "text": "DOWN ‚ùå" } }, "type": "value" },
            { "options": { "1": { "color": "green", "index": 0, "text": "UP ‚úÖ" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Request rate per service (excluding actuator endpoints)",
